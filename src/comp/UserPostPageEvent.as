package comp
{
	import flash.events.Event;
	
	public class UserPostPageEvent extends Event
	{
		
		public var user_id:String;
		
		public function UserPostPageEvent(type:String, _user_id:String, bubbles:Boolean=false, cancelable:Boolean=false)
		{
			super(type, bubbles, cancelable);
			user_id = _user_id;
		}
	}
}